{
  "expo": {
    "name": "Alzhistant",
    "slug": "Alzhistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBZLQe6igFf5G2h93D-nRPcJ2bFs0HXdPc"
        }
      },
      "permissions": [
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "alzhistatn.apk"
    }
  }
}
